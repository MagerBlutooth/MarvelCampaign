<?import adventure.view.node.ProfileNode?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<fx:root type="ProfileNode"
         xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1"
         stylesheets="@../css_adventure/mainStyle.css" fx:controller="adventure.controller.manager.ProfileNodeController">
    <StackPane id="profilePane" prefWidth="800" prefHeight="300">
        <AnchorPane>
            <Label fx:id="profileNumber" textFill="ghostwhite" opacity="0.8" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="0.0"/>
        </AnchorPane>
        <Label fx:id="profileText" textFill="ghostwhite" opacity="0.8"/>
    </StackPane>
    <padding>
        <Insets left="20.0"/>
    </padding>
</fx:root>