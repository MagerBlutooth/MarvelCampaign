<?import adventure.view.node.ProfileNode?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<fx:root type="ProfileNode"
         xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1"
         stylesheets="@../css_adventure/mainStyle.css" fx:controller="adventure.controller.manager.ProfileNodeController">
    <StackPane id="profilePane" prefWidth="800" prefHeight="300">
        <VBox spacing="10.0">
            <HBox spacing="10.0">
                <Label fx:id="profileNumberLabel" textFill="ghostwhite" opacity="0.5"/>
                <Label fx:id="profileText" textFill="white"/>
            </HBox>
            <VBox fx:id="contentBox" AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                  AnchorPane.rightAnchor="0.0"/>
        </VBox>
    </StackPane>
    <padding>
        <Insets left="20.0"/>
    </padding>
</fx:root>