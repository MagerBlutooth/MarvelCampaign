<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<fx:root type="javafx.scene.layout.StackPane" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1"
         stylesheets="@../css_adventure/mainStyle.css" fx:controller="adventure.controller.LogViewPaneController"
         prefWidth="800" prefHeight="800">
    <VBox alignment="CENTER" spacing="30.0">
        <Label text="Adventure Log"/>
        <ScrollPane fx:id="logScrollPane" fitToHeight="true" fitToWidth="true"/>
        <Button fx:id="backButton" text="Back" onAction="#goBack"/>
    </VBox>
    <padding>
        <Insets top="20.0" bottom="20.0" left="20.0" right="10.0"/>
    </padding>
</fx:root>