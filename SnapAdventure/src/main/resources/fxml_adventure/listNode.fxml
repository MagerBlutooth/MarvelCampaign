<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.*?>
<?import snapMain.view.node.GridDisplayNode?>
<fx:root type="javafx.scene.layout.StackPane" fx:id="mainPane"
         xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1"
         stylesheets="@../css_adventure/mainStyle.css"
         prefWidth="1200" prefHeight="800">
    <HBox alignment="CENTER">
        <VBox fx:id="contentBox" alignment="CENTER" spacing="20.0">
            <GridDisplayNode fx:id="list" stylesheets="@../css_adventure/mainStyle.css">
                <padding>
                    <Insets top="20" bottom="20" right="20" left="20"/>
                </padding>
            </GridDisplayNode>
        </VBox>
    </HBox>
    <padding>
        <Insets top="20.0" bottom="20.0" left="20.0" right="10.0"/>
    </padding>

</fx:root>
