<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import campaign.view.button.ButtonToolBar?>
<fx:root prefHeight="600" prefWidth="600" stylesheets="@../css/mainStyle.css" type="campaign.view.pane.LoadSelectPane" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="campaign.controller.LoadSelectPaneController">
    <AnchorPane>
        <VBox fx:id="mainBox" alignment="CENTER" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0">
            <HBox alignment="CENTER" spacing="20.0">
                <Button onAction="#loadPlayer" text="Player" />
                <Button onAction="#loadWatcher" text="Watcher" />
            </HBox>
        </VBox>
        <ButtonToolBar fx:id="buttonToolBar" alignment="TOP_CENTER" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
    </AnchorPane>
</fx:root>
