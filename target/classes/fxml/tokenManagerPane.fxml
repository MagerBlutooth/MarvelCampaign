<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import view.button.ButtonToolBar?>
<?import view.manager.TokenManager?>
<?import view.pane.manager.TokenManagerPane?>
<fx:root type="view.pane.manager.TokenManagerPane"
         xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1"
         stylesheets="@../css/mainStyle.css" fx:controller="controller.grid.TokenManagerPaneController"
         prefWidth="1200" prefHeight="800">
    <VBox alignment="CENTER">
        <ButtonToolBar fx:id="buttonToolBar"/>
        <HBox spacing="30.0" alignment="CENTER">
            <VBox alignment="CENTER">
                <Button text="Add New" onAction="#addNewEntry"/>
                <TokenManager fx:id="tokenManager"/>
            </VBox>
        </HBox>
    </VBox>
    <padding>
        <Insets top="20.0" bottom="20.0" left="20.0" right="10.0"/>
    </padding>
</fx:root>